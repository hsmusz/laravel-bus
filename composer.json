{
  "name": "mr-luke/bus",
  "description": "Multi-purpose bus for Laravel",
  "keywords": ["bus", "laravel"],
  "type": "library",
  "license": "MIT",
  "verion": "2.0.0",
  "authors": [
    {
      "name": "≈Åukasz Sitnicki",
      "email": "lukasz.sitnicki@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "laravel/framework": "^12.0|^11.0|^10.0|^9.0|^8.0",
    "mr-luke/configuration": "^1.0",
    "nesbot/carbon": "^3.8"
  },
  "require-dev": {
    "fakerphp/faker": "*",
    "mockery/mockery": "*",
    "orchestra/testbench": "*",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Mrluke\\Bus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mrluke\\Bus\\BusServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
