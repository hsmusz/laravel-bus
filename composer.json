{
  "name": "mr-luke/bus",
  "description": "Multi-purpose bus for Laravel",
  "keywords": ["bus", "laravel"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "≈Åukasz Sitnicki",
      "email": "lukasz.sitnicki@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "ext-json": "*",
    "illuminate/bus": "^9.0|^8.0|^7.0|^6.0",
    "illuminate/database": "^9.0|^8.0|^7.0|^6.0",
    "illuminate/support": "^9.0|^8.0|^7.0|^6.0",
    "mr-luke/configuration": "^1.0",
    "nesbot/carbon": "^2.16.0"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.0",
    "orchestra/testbench": "^7.0|^6.0|^5.0|^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Mrluke\\Bus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mrluke\\Bus\\BusServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
